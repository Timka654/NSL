{
  "$schema": "http://json.schemastore.org/template",
  "author": "relife87",
  "classifications": [ "Common", "Library" ],
  "identity": "Shared.Combine.Library",
  "name": "Share Combined Library(Server/Client)",
  "shortName": "sharedcombolib",
  "sourceName": "Shared.Combine.Library",
  "defaultName": "Project.Shared",
