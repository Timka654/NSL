    "language": "C#",
    "type": "project"
  },
  "symbols": {
    "Framework": {
      "type": "parameter",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net6.0"
        },
        {
          "choice": "net7.0"
        },
        {
          "choice": "net8.0"
        }
      ],
      "defaultValue": "net8.0",
      "replaces": "{TargetFramework}"
    }
  }
}