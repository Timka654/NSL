	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='UnityDebug|AnyCPU'">
		<DefineConstants>$(DefineConstants);DEVELOP</DefineConstants>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<DefineConstants>$(DefineConstants);DEVELOP</DefineConstants>
	</PropertyGroup>

