<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<Configurations>Debug;Release;Unity;UnityDebug;DebugStable</Configurations>
		<Version>$(VersionSuffix)</Version>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>
		<AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
		<GenerateBindingRedirectsOutputType>true</GenerateBindingRedirectsOutputType>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)'=='Debug'">
		<TargetFramework>net5.0</TargetFramework>
		<PackageId>$(MSBuildProjectName)_Debug</PackageId>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)'=='Release'">
		<TargetFramework>net5.0</TargetFramework>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Unity|AnyCPU'">
		<TargetFramework>netstandard2.0</TargetFramework>
		<OutputPath>..\UnityDebug\</OutputPath>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='UnityDebug|AnyCPU'">
		<TargetFramework>netstandard2.0</TargetFramework>
		<DefineConstants>DEBUG;TRACE</DefineConstants>
		<!--<PackageId>$(AssemblyName).Develop</PackageId>-->
		<OutputPath>..\UnityDebug\</OutputPath>
	</PropertyGroup>

	<ItemGroup>
	  <ProjectReference Include="..\SocketCore.Extensions.Packet\SocketCore.Extensions.Packet.csproj" />
	</ItemGroup>

	<!--<ItemGroup>
		<ProjectReference Include="..\SocketCore.Extensions.Packet\SocketCore.Extensions.Packet.csproj" />
	</ItemGroup>-->

</Project>
