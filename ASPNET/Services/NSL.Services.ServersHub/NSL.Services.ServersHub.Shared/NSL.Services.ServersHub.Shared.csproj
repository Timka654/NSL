<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Library</OutputType>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<PropertyGroup>
		<DefineConstants>$(DefineConstants);CLIENT</DefineConstants>
	</PropertyGroup>

	<ItemGroup>
		<Compile Remove="artifacts\**" />
		<Content Remove="artifacts\**" />
		<EmbeddedResource Remove="artifacts\**" />
		<None Remove="artifacts\**" />

		<Compile Remove="Server\**" />
		<Content Remove="Server\**" />
		<EmbeddedResource Remove="Server\**" />
		<None Remove="Server\**" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="NSL.Generators.HttpEndPointGenerator.Shared" Version="2024.7.29.126" />
	  <PackageReference Include="NSL.HttpClient" Version="2024.7.29.126" />
	  <PackageReference Include="NSL.Refactoring.Shared" Version="2024.7.29.126">
	    <PrivateAssets>all</PrivateAssets>
	    <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
	  </PackageReference>
	</ItemGroup>

	<ItemGroup>
	  <Folder Include="Client\Models\" />
	</ItemGroup>

</Project>
